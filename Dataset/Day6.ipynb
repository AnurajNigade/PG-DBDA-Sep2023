   "source": [
    "def logistic(x):\n",
    "    return np.exp(x) / (1 + np.exp(x))\n",
    "\n",
    "x = np.linspace(-6,6,50, dtype=float)\n",
    "\n",
    "y = logistic(x)\n",
    "\n",
    "plt.plot(x,y)\n"
   ]
